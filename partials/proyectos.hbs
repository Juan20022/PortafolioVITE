<section class="card-container">
    <div class="titulo">
        <h1>Mis Proyectos</h1>
    </div>
    <div class="card-row">
        {{#each proyectos}}
        <div class="card">
            <div class="card-image">
                <img alt="{{title}}" src="{{img}}" />
            </div>
            <div class="card-title">
                <h2>{{title}}</h2>
            </div>
        </div>
        {{/each}}
    </div>
</section>
